		"lint": "eslint 'src/**/**.{js,ts}'",
		"lint:fix": "eslint 'src/**/**.{js,ts}' --fix",
		"format": "prettier 'src/**/**.{js,ts}' --write",
		"format:check": "prettier 'src/**/**.{js,ts}' --check",
		"mrm": "mrm --preset @pinetwork-js/mrm-preset"
	},
	"packageManager": "yarn@3.2.2",
	"author": "Brewal Derouet <brewal_derouet@hotmail.fr>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PiNetwork-js/sdk.git"
	},
	"homepage": "https://github.com/PiNetwork-js/sdk#readme",
	"bugs": {
		"url": "https://github.com/PiNetwork-js/sdk/issues"
	},
	"dependencies": {
		"axios": "^0.27.2"
	},
	"devDependencies": {
		"@microsoft/tsdoc": "^0.14.1",
		"@pinetwork-js/api-typing": "^0.4.0",
		"@pinetwork-js/eslint-config": "^0.2.0",
		"@pinetwork-js/mrm-preset": "^0.2.1",
		"@pinetwork-js/prettier-config": "^0.1.0",
		"@types/node": "^18.6.3",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-node": "^0.3.6",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-sonarjs": "^0.14.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"eslint-plugin-unicorn": "^43.0.2",
		"mrm": "^4.0.0",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	},
	"files": [
		"build"
	],
	"keywords": [
		"pi",
